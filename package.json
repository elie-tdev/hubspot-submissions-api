{
  "name": "hubspot-submissions-api",
  "description": "A Custom React Typescript hooks for interacting with Hubspot Submissions API",
  "version": "1.0.1",
  "main": "index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:topfullstackdev/hubspot-submissions-api.git"
  },
  "keywords": [
    "react",
    "typescript",
    "react typescript",
    "react-hook",
    "submissions",
    "form",
    "hubspot",
    "hubspot-api",
    "hubspot-form",
    "hubspot-submissions"
  ],
  "author": "Jouji Yoshino",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  }
}
